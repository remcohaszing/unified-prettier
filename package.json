  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "c8": "^9.0.0",
    "eslint": "^8.0.0",
    "eslint-config-remcohaszing": "^10.0.0",
    "remark-cli": "^11.0.0",
    "remark-preset-remcohaszing": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
